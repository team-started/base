{
    "name": "starterteam/base",
    "description": "TYPO3 CMS Distribution with TYPO3-Console and Surf support",
    "authors": [
        {
            "name": "Christian Wolfram",
            "email": "c.wolfram@chriwo.de"
        },
        {
            "name": "Marc Fell",
            "email": "marc@fell.hamburg"
        },
        {
            "name": "Marcel Schmid",
            "email": "m.schmid@visual-fx.ch"
        },
        {
            "name": "Jens Seiller",
            "email": "buero@jdd-webdesign.de"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.1",
        "cweagans/composer-patches": "^1.7",
        "georgringer/news": "^9.4",
        "helhum/dotenv-connector": "^3.0",
        "helhum/typo3-console": "^7.1",
        "ichhabrecht/content-defender": "^3.2",
        "pti/pti": "dev-feature/support_t3v11 || dev-master",
        "starterteam/site-package-freya": "dev-main",
        "starterteam/site-package-nessa": "dev-main",
        "starterteam/starter": "dev-main",
        "starterteam/starter-freya": "dev-main",
        "starterteam/starter-nessa": "dev-main",
        "starterteam/starter-twig": "dev-main",
        "starterteam/starter-twig-news": "dev-main",
        "twig/twig": "^3",
        "typo3/cms-backend": "^11.5",
        "typo3/cms-belog": "^11.5",
        "typo3/cms-beuser": "^11.5",
        "typo3/cms-core": "^11.5",
        "typo3/cms-extbase": "^11.5",
        "typo3/cms-extensionmanager": "^11.5",
        "typo3/cms-filelist": "^11.5",
        "typo3/cms-fluid": "^11.5",
        "typo3/cms-fluid-styled-content": "^11.5",
        "typo3/cms-form": "^11.5",
        "typo3/cms-frontend": "^11.5",
        "typo3/cms-impexp": "^11.5",
        "typo3/cms-info": "^11.5",
        "typo3/cms-install": "^11.5",
        "typo3/cms-lowlevel": "^11.5",
        "typo3/cms-recordlist": "^11.5",
        "typo3/cms-redirects": "^11.5",
        "typo3/cms-reports": "^11.5",
        "typo3/cms-rte-ckeditor": "^11.5",
        "typo3/cms-scheduler": "^11.5",
        "typo3/cms-seo": "^11.5",
        "typo3/cms-setup": "^11.5",
        "typo3/cms-sys-note": "^11.5",
        "typo3/cms-t3editor": "^11.5",
        "typo3/cms-tstemplate": "^11.5",
        "typo3/cms-viewpage": "^11.5"
    },
    "require-dev": {
        "michielroos/typo3scan": "^1.7",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.9",
        "saschaegerer/phpstan-typo3": "^1.8",
        "ssch/typo3-rector": "^1.1",
        "symplify/monorepo-builder": "^10.0",
        "typo3/coding-standards": "^0.6.1"
    },
    "autoload": {
        "psr-4": {
            "StarterTeam\\StarterFreya\\": "packages/starter_freya/Classes",
            "StarterTeam\\StarterNessa\\": "packages/starter_nessa/Classes",
            "StarterTeam\\StarterTwigNews\\": "packages/starter_twig_news/Classes",
            "StarterTeam\\StarterTwig\\": "packages/starter_twig/Classes",
            "StarterTeam\\Starter\\": "packages/starter/Classes"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages/*"
        },
        {
            "type": "composer",
            "url": "https://packagist.starter.team"
        },
        {
            "type": "vcs",
            "url": "https://github.com/chriwo/typo3-pti.git"
        }
    ],
    "extra": {
        "typo3/cms": {
            "extension-key": [
                "customer_sitepackage",
                "starter_freya",
                "starter_twig",
                "sitepackage_nessa",
                "starter",
                "starter_nessa",
                "starter_twig_news",
                "sitepackage_freya"
            ],
            "cms-package-dir": "{$vendor-dir}/typo3/cms"
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patches": {
            "pti/pti": {
                "Prepend site base uri to assets": "patches/pti/pti_prepend_site_uri_assets.patch",
                "Fix allowed data type for image processing": "patches/pti/data_types_image_processing.patch",
                "Make file collection records renderable": "patches/pti/make_file_collections_renderable.patch",
                "Preview image rendering runs into exception": "patches/pti/preview_image_runs_into_exception.patch"
            }
        }
    },
    "config": {
        "secure-http": true,
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "ext-dom": "1",
            "ext-intl": "1",
            "ext-simplexml": "1",
            "ext-xml": "1",
            "ext-xmlwriter": "1",
            "ext-json": "1",
            "ext-gd": "1"
        },
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true,
            "helhum/dotenv-connector": true,
            "cweagans/composer-patches": true,
            "phpstan/extension-installer": true
        }
    },
    "scripts": {
        "ci:php:cs": [
            "php-cs-fixer fix --config=../../.build/tools/.php-cs-fixer.php -v --dry-run --using-cache no --diff",
            "php-cs-fixer fix --config=../../.build/tools/.php-cs-fixer-nonclasses.php -v --dry-run --using-cache no --diff"
        ],
        "ci:php:cs-fix": [
            "php-cs-fixer fix --config=../../.build/tools/.php-cs-fixer.php -v --using-cache no",
            "php-cs-fixer fix --config=../../.build/tools/.php-cs-fixer-nonclasses.php -v --using-cache no"
        ],
        "ci:php:stan": [
            "@ci:php:stan8.1"
        ],
        "ci:php:phpstan-baseline": [
            "@ci:php:phpstan-baseline8.1"
        ],
        "ci:php:stan8.1": [
            "phpstan --configuration=../../.build/tools/phpstan.neon"
        ],
        "ci:php:phpstan-baseline8.1": [
            "phpstan --configuration=../../.build/tools/phpstan.neon --generate-baseline=../../.build/tools/phpstan-baseline.neon"
        ],
        "ci:php:stan7.4": [
            "phpstan --configuration=../../.build/tools/phpstan-74.neon"
        ],
        "ci:php:phpstan-baseline7.4": [
            "phpstan --configuration=../../.build/tools/phpstan-74.neon --generate-baseline=../../.build/tools/phpstan-baseline-74.neon"
        ],
        "ci:php:rector": [
            "@ci:php:rector8.1"
        ],
        "ci:php:rector8.1": [
            "rector process --dry-run --clear-cache --config=../../.build/tools/rector.php"
        ],
        "ci:php:rector7.4": [
            "rector process --dry-run --clear-cache --config=../../.build/tools/rector-74.php"
        ],
        "create-symlinks": [
            "ln -sf vendor/helhum/typo3-console/typo3cms typo3cms"
        ],
        "post-install-cmd": [
            "@create-symlinks"
        ],
        "post-update-cmd": [
            "@create-symlinks"
        ]
    },
    "replace": {
        "starterteam/customer-site-package": "dev-main",
        "starterteam/site-package-freya": "dev-main",
        "starterteam/site-package-nessa": "dev-main",
        "starterteam/starter": "dev-main",
        "starterteam/starter-freya": "dev-main",
        "starterteam/starter-nessa": "dev-main",
        "starterteam/starter-twig": "dev-main",
        "starterteam/starter-twig-news": "dev-main"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
