{
    "name": "starterteam/site-package-freya",
    "type": "typo3-cms-extension",
    "description": "StarterTeam sitepackage demo extension for EXT:starter-freya",
    "license": "proprietary",
    "require": {
		"php": "^8.2",
		"typo3/cms-core": "^12.4 || ^13.0",
		"starterteam/starter": "^4.5 || dev-main",
		"starterteam/starter-freya": "^4.5 || dev-main"
	},
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "sitepackage_freya",
            "app-dir": ".Build",
            "web-dir": ".Build/public"
        }
    }
}
