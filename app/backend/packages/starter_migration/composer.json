{
    "name": "starterteam/starter-migration",
    "type": "typo3-cms-extension",
    "description": "StarterTeam content element migrations",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
		"starterteam/starter": "dev-main",
        "typo3/cms-core": "^11.5"
	},
	"require-dev": {
		"michielroos/typo3scan": "^1.7",
		"nimut/testing-framework": "^6.0",
		"phpstan/extension-installer": "^1.2",
		"phpstan/phpstan": "^1.8",
		"phpstan/phpstan-phpunit": "^1.1",
		"phpunit/phpunit": "^9.5",
		"rector/rector": "^0.13.4",
		"saschaegerer/phpstan-typo3": "^1.8",
		"typo3/coding-standards": "^0.5.0"
	},
	"autoload": {
		"psr-4": {
			"StarterTeam\\StarterMigration\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"StarterTeam\\StarterMigration\\Tests\\": "Tests/"
		}
	},
    "extra": {
		"branch-alias": {
			"dev-main": "4.x-dev"
		},
        "typo3/cms": {
            "extension-key": "starter_migration"
        }
    }
}
