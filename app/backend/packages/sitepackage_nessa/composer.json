{
    "name": "starterteam/site-package-nessa",
    "type": "typo3-cms-extension",
    "description": "StarterTeam sitepackage extension for EXT:starter-nessa",
    "license": "proprietary",
    "require": {
        "php": "^7.4 || ^8.1",
        "typo3/cms-core": "^11.5",
        "starterteam/starter": "^4.4",
        "starterteam/starter-nessa": "^4.4"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "sitepackage_nessa",
            "app-dir": ".Build",
            "web-dir": ".Build/public"
        }
    }
}
